   "source": [
    "\n",
    "class Solution:\n",
    "    # @param A : list of integers\n",
    "    # @return a list of integers\n",
    "    def wave(self, A):\n",
    "        A = sorted(A)\n",
    "        for i in range(0,len(A)-1,2):\n",
    "            A[i], A[i+1] = A[i+1], A[i]\n",
    "        return A\n",
    "    \n",
    "    \n",
    "s = Solution()    \n",
    "A = [1, 5, 7, 3]\n",
    "s.wave(A)"
   ]
